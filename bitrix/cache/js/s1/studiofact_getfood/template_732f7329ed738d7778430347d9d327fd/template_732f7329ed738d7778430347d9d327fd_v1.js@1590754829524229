function EmptyBasket() {
    var $basketContainer = $('#update_big_basket_ajax .box');

    if ($basketContainer.length) {
	    $basketContainer.empty();
	    $basketContainer.append(BX.message('SALE_EMPTY_BASKET'));
    }
}


